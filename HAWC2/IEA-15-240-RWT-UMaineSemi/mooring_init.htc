;-----------------------------------------------------------------------------------------
; OUTPUT
;-----------------------------------------------------------------------------------------
begin output;
  filename ./res/[flnm] ;
  data_format  hawc_ascii;
  buffer 1 ;
  time 100.0 100.0 ;
;
  general time;  
;  constraint bearing1 shaft_rot 2; angle and angle velocity 
;  constraint bearing2 pitch1 5;    angle and angular velocity 
;  constraint bearing2 pitch2 5;    angle and angular velocity 
;  constraint bearing2 pitch3 5;    angle and angular velocity 
;  aero omega ;
;  aero torque;
;  aero power;
;  aero thrust;
;  wind free_wind 1 0.0 0.0 -150; local wind at fixed position: coo (1=global,2=non-rotation rotor coo.), pos x, pos y, pos z
; Moments:
;  mbdy momentvec tower  1 1  tower # tower base ;
;  mbdy momentvec tower  10 2 tower # tower yaw bearing ;
;  mbdy momentvec shaft  1 1  shaft # main bearing ;
;  mbdy momentvec blade1 2  2 blade1 # blade 1 root flap-edge ;
;  mbdy momentvec blade2 2  2 blade2 # blade 2 root flap-edge ;
;  mbdy momentvec blade3 2  2 blade3 # blade 3 root flap-edge ;
  ;
;  mbdy momentvec blade1 2  2 hub1 # blade 1 root in-out-of-plane ;
;  mbdy momentvec blade2 2  2 hub2 # blade 2 root in-out-of-plane ;
;  mbdy momentvec blade3 2  2 hub3 # blade 3 root in-out-of-plane ;
; Displacements and accellerations
;  mbdy state pos tower 10 1.0 global only 2 # Tower top FA displ - global; 
;  mbdy state pos tower 10 1.0 global only 1 # Tower top SS displ - global; 
;  mbdy state vel tower 10 1.0 global only 2 # Tower top FA veloc - global; 
;  mbdy state vel tower 10 1.0 global only 1 # Tower top SS veloc - global; 
;  mbdy state acc tower 10 1.0 global only 2 # Tower top FA acc - global; 
;  mbdy state acc tower 10 1.0 global only 1 # Tower top SS acc - global; 
  ;  
;  mbdy state pos tower 10 1.0 tower only 2 # Tower top FA displ - tower; 
;  mbdy state pos tower 10 1.0 tower only 1 # Tower top SS displ - tower;
;  mbdy state vel tower 10 1.0 tower only 2 # Tower top FA veloc - tower; 
;  mbdy state vel tower 10 1.0 tower only 1 # Tower top SS veloc - tower;
;  mbdy state acc tower 10 1.0 tower only 2 # Tower top FA acc - tower; 
;  mbdy state acc tower 10 1.0 tower only 1 # Tower top SS acc - tower;
  ;
;  mbdy state pos blade1  19 1.0 blade1 # blade 1 tip pos ;
;  mbdy state pos blade2  19 1.0 blade2 # blade 2 tip pos ;
;  mbdy state pos blade3  19 1.0 blade3 # blade 3 tip pos ;
;  mbdy state pos blade1  19 1.0 global # gl blade 1 tip pos ;  
; - Monitor Aerodynamics - ;
;  aero windspeed 3 1 1 72.5;
;  aero alfa 1 72.5; 
;  aero alfa 2 72.5; 
;  aero alfa 3 72.5; 
;  aero cl 1 72.5;
;  aero cl 2 72.5;
;  aero cl 3 72.5;
;  aero cd 1 72.5;
;  aero cd 2 72.5;
;  aero cd 3 72.5;   
;
